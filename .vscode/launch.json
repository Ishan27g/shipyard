  "version": "0.2.0",
  "configurations": [

    {
      "name": "Debug - Up",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      "env": {
        "LOG_LEVEL": "debug"
      },
      "args": [
        "up",
        "./examples/docs",
      ]
    },    {
      "name": "Debug - Down",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      "env": {
        "LOG_LEVEL": "debug"
      },
      "args": [
        "down"
      ]
    },
    {
      "name": "Debug - Push",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      "env": {
        "LOG_LEVEL": "debug"
      },
      "args": [
        "push",
        "nicholasjackson/example-wasm-filter:latest",
        "k8s_cluster.k3s"
      ]
    },
    {
      "name": "Debug - Test",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      "env": {
        "LOG_LEVEL": "debug"
      },
      "args": [
        "test",
        "./examples/build",
      ]
    }
  ]
}